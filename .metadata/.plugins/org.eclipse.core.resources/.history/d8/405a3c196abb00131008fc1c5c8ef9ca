Ti.UI.setBackgroundColor("#fff");

var mainWindow = Ti.UI.createWindow({
	
	backgroundColor: "#fff",
	backgroundImage: "CustomBG.png",
	backgroundRepeat: true,
	top: 20,
});

var myFirstView = Ti.UI.createView({
	backgroundColor: "#fff",
	borderColor: "#000",
	borderRadius: 5,
	borderWidth: 1,
	top: 30,
	left: 20,
	right: 20,
	bottom: 120
});

var mySecondView = Ti.UI.createView({
	backgroundColor: "#000",
	borderColor: "#000",
	borderRadius: 5,
	borderWidth: 1,
	height: 50,
	width: 75,
	bottom: 50,
	left: 40,
});

var myThirdView = Ti.UI.createView({
	backgroundColor: "#000",
	borderColor: "#000",
	borderRadius: 5,
	borderWidth: 1,
	height: 50,
	width: 75,
	bottom: 50,
	right: 40,
});
var myFourthView = Ti.UI.createView({
	backgroundColor: "#fff",
	borderColor: "#000",
	borderRadius: 5,
	borderWidth: 1,
	top: 600,
	bottom: 20
	});

var myText = Ti.UI.createLabel({
	text:"Previous",
	color:"red",
	font: {fontSize: 14, fontFamily: "Helvetica", fontWeight: "bold", fontStyle: "italic"}
});

var mySecondText = Ti.UI.createLabel({
	text:"Next",
	color:"red",
	font: {fontSize: 14, fontFamily: "Helvetica", fontWeight: "bold", fontStyle: "italic"}
});

mainWindow.add(myFirstView,myFourthView,mySecondView,myThirdView);
mySecondView.add(myText);
myThirdView.add(mySecondText);
mainWindow.open();
