Ti.UI.setBackgroundColor("#fff");

var mainWindow = Ti.UI.createWindow({
	
	backgroundColor: "#fff",
	top: 20
});

var myView = Ti.UI.createView({
	backgroundColor: "#fff",
	borderColor: "#000",
	borderRadius: 5,
	borderWidth: 1,
	top: 30,
	left: 20,
	right: 20,
	bottom: 120
});

win.add(myView);
win.open();
