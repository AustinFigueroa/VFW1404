Ti.UI.setBackgroundColor("#fff");

var win = Ti.UI.createWindow({
	title: "Image Gallery",
	backgroundColor: "#f5f5f5",
	modal: true,
	layout: "horizontal"
});

for (var i=0, j=10; i<j; i++){
	var aImage = Ti.UI.createImageView({
		image: "Image/GroupAnime.jpg",
		borderRadius: 5,
		top: 15,
		left: 15,
		width: 75,
		caption: "Naruto Family"
	});
	win.add(aImage);
}


win.open();
