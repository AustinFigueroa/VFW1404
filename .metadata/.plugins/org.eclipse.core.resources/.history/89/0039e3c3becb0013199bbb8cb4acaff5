var win = Ti.UI.createWindow({
	title: "Image Gallery",
	backgroundColor: "#f5f5f5",
	modal: true,
	layout: "horizontal"
});

for (var i=0, j=10; i<j; i++){
	var imageWrapper = Ti.UI.createView({
		borderRadius: 5,
		top: 15,
		left: 15,
		width: 75,
		height: 75,
		backgroundColor: "#fff",
		layout:"vertical"
	});
	var aImage = Ti.UI.createImageView({
		image: "Image/GroupAnime.jpg",
		caption: "Naruto Family"
	});
	var imageLabel = Ti.UI.createLabel({
		text: aImage.caption,
		top: 5,
		font: {fontSize: 10}
	});
	imageWrapper.add(aImage);
	win.add(imageWrapper);
}


win.open();
