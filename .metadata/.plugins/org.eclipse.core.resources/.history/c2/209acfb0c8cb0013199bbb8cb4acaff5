Ti.UI.setBackgroundColor("#fff");

var win = Ti.UI.createWindow({
	backgroundColor: "#333",
	top: 20
});

var myView = Ti.UI.createView({
	backgroundColor: "#fff",
	borderColor: "#000",
	borderRadius: 5,
	borderWidth: 1,
	align: "center",
	top: 200,
	left: 20,
	right: 20,
	bottom: 200
});

var myText = Ti.UI.createLabel({
	text:"Open Gallery",
	color:"#000",
	font: {fontSize: 30, fontFamily: "Arial"}
});

myView.add(myText);
win.add(myView);
win.open();
