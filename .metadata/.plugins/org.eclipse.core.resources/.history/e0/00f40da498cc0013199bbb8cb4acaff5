var win = Ti.UI.createWindow({
backgroundColor: "#333",
top: 20
});

var myView = Ti.UI.createView({
backgroundColor: "#fff",
borderColor: "#000",
borderRadius: 5,
borderWidth: 1,
align: "center",
top: 225,
left: 20,
right: 20,
bottom: 225
});

var myText = Ti.UI.createLabel({
text:"Open Gallery",
color:"#000",
font: {fontSize: 24, fontFamily: "Arial"}
});
var getImages = function(){
var imageWin = Ti.UI.createWindow({
title: "Image Gallery",
font: {fontSize:14, fontFamily: "Arial", fontColor: "#000"},
    backgroundColor: "#fff"
});

var closeButton = Ti.UI.createLabel({
text: "Close Window",
backgroundColor: "#333",
color: "#fff",
height: 50,
font:{fontSize: 14, fontFamily: "Arial"},
width: "100%",
bottom: 0,
textAlign: "center"
});
var closeWindow = function(){
imageWin.close();
};
imageWin.open();


closeButton.addEventListener("click", closeWindow);
imageWin.add(closeButton);
};
win.open();
